package com.openrun.ticket.user;

//구매자 Vo
public class UserVo {
	
	int user_no; //구매자 회원번호
	String u_id; //구매자 아이디
	String u_pw; //구매자 비밀번호
	String u_name; //구매자 이름
	int u_birth; //구매자 생년월일
	String u_address; //구매자 주소
	int u_phone; //구매자 휴대폰번호
	String u_email; //구매자 이메일
	String u_bank_name; //구매자 은행명
	int u_account_no; //구매자 계좌번호
	String u_join_date;// 구매자 회원가입일자
}
